<h1 class="jumbotron text-center">Scrape your News!</h1>

<div class="container">
    {{#each stories}}
      <div class="well">
        <div style="background-color:cyan">
          {{this.title}}
          <button class="btn btn-warning text-right" data-id="{{this._id}}">
            Save Article 
          </button>
        </div>
        <br>
        <a href={{this.link}}>{{this.link}}</a>
      </div>
    {{/each}}
</div>


